{
    "name": "Idris Elba img replacer",
    "description": "Replaces all images with Idris Elba",
    "version": "0.1.0",
    "manifest_version": 2,
    "web_accessible_resources": [
        "idris.jpg"
      ],
    "content_scripts": [
        {
            "matches":["*://*/*"],
            "js": ["content.js"]
        }
    ]

}